<?xml version="1.0"?>
<odoo>
    <data>
        <record id="view_purchase_order_coopsol_inherit" model="ir.ui.view">
            <field name="name">purchase.order.coopsol.inherit</field>
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='partner_id']" position="after">
                    <field name="contact_partner_id" domain="[('commercial_partner_id','=',partner_id)]"/>
                </xpath>
                <xpath expr="//field[@name='user_id']" position="before">
                    <field name="quot_number"/>
                    <field name="contact_id"/>
                    <field name="analytic_account_id"/>
                    <field name="ambassador_id"/>
                </xpath>
                <xpath expr="//notebook/page[@name='purchase_delivery_invoice']" position="inside">
                    <field name="obs" class="oe_inline" placeholder="Observaciones ..."/>
                    <div class="oe_clear"/>
                </xpath>
            </field>
        </record>
	</data>
</odoo>