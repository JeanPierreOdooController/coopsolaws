<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="error_404" model="ir.ui.view">
            <field name="name">404 Error no se encuentra</field>
            <field name="type">qweb</field>
            <field name="key">website.error_404</field>
            <field name="arch" type="xml">
                <t name="Error 404" t-name="website.error_404">
					<t t-call="website.layout">
					<div id="wrap" class="oe_structure oe_empty">
						<section class="s_faq_collapse pt48 pb32" data-name="Accordion">
						<div class="container">
							<div class="row s_nb_column_fixed">
							<div class="col-lg-12 s_title pt16 pb16">
								<h1 class="s_title_thin">
								<font style="font-size: 62px;">ERROR 404 PAGINA NO ENCONTRADA</font>
								</h1>
							</div>
							</div>
						</div>
						</section>
					</div>
					</t>
                </t>
            </field>
        </record>
		<record id="error_404_page" model="website.page">
            <field name="is_published">True</field>
            <field name="custom_field">True</field>
            <field name="url">/error_404</field>
            <field name="view_id" ref="error_404"/>
            <field name="track">True</field>
        </record>
	</data>
</odoo>
